{
    "name": "api-clients/openapi-client-generator",
    "description": "Generate a client based on an OpenAPI spec",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "bin": [
        "bin/openapi-client-generator"
    ],
    "extra": {
        "wyrihaximus": {
            "bin-autoload-path-update": [
                "bin/openapi-client-generator"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "devizzent/cebe-php-openapi": "^1",
        "jawira/case-converter": "^3.5",
        "twig/twig": "^3.5",
        "nikic/php-parser": "^4.15",
        "psr/http-message": "^1.0",
        "ringcentral/psr7": "^1.3",
        "symfony/yaml": "^5.4",
        "wyrihaximus/composer-update-bin-autoload-path": "^1 || ^1.0.1",
        "league/openapi-psr7-validator": "dev-master as 0.19",
        "react/http": "^1.8",
        "reactivex/rxphp": "^2.0",
        "api-clients/contracts": "^0.1",
        "eventsauce/object-hydrator": "^1.2",
        "react/async": "^4.0",
        "wyrihaximus/react-awaitable-observable": "^1.0",
        "ckr/arraymerger": "^3.0",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "ApiClients\\Tools\\OpenApiClientGenerator\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.13"
        },
        "allow-plugins": {
            "wyrihaximus/composer-update-bin-autoload-path": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiClients\\Tests\\Tools\\OpenApiClientGenerator\\": "tests/"
        }
    }
}
